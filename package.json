{
  "name": "design-system-icons",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@4.10.3",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "ci": "yarn install --frozen-lockfile",
    "optimize-icons": "node scripts/optimize-icons.mjs",
    "generate:icons": "node scripts/generate-icons-map.mjs",
    "generate:tokens": "node scripts/generate-design-tokens.mjs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "verify:agents": "node scripts/verify-agents-version.mjs",
    "storybook:react": "cross-env STORYBOOK_REF_MODE=dev STORYBOOK_COMPOSE_ANGULAR=false STORYBOOK_COMPOSE_VUE=false storybook dev -p 6006",
    "storybook:react:compose": "cross-env STORYBOOK_REF_MODE=dev STORYBOOK_COMPOSE_ANGULAR=true STORYBOOK_COMPOSE_VUE=true storybook dev -p 6006",
    "prestorybook:react": "yarn run generate:tokens",
    "storybook:angular": "cross-env STORYBOOK_REF_MODE=dev storybook dev -p 6007 -c storybooks/angular/.storybook",
    "prestorybook:angular": "yarn run generate:tokens",
    "storybook:vue": "storybook dev -p 6008 -c storybooks/vue/.storybook",
    "prestorybook:vue": "yarn run generate:tokens",
    "storybook": "concurrently --kill-others-on-fail --names angular,vue,react \"yarn storybook:angular\" \"yarn storybook:vue\" \"wait-on tcp:6007 tcp:6008 && yarn storybook:react:compose\"",
    "prestorybook": "yarn run generate:tokens",
    "build-storybook": "yarn run build-storybook:compose",
    "prebuild-storybook": "yarn run generate:icons && yarn run generate:tokens",
    "build-storybook:root": "cross-env STORYBOOK_REF_MODE=static storybook build",
    "build-storybook:angular": "cross-env STORYBOOK_REF_MODE=static storybook build -c storybooks/angular/.storybook -o storybooks/angular/storybook-static",
    "prebuild-storybook:angular": "yarn run generate:icons && yarn run generate:tokens",
    "build-storybook:vue": "storybook build -c storybooks/vue/.storybook -o storybooks/vue/storybook-static",
    "prebuild-storybook:vue": "yarn run generate:icons && yarn run generate:tokens",
    "build-storybook:compose": "yarn run build-storybook:angular && yarn run build-storybook:vue && yarn run build-storybook:root && node scripts/copy-storybook-refs.mjs",
    "previsual:test": "playwright install --with-deps chromium chromium-headless-shell",
    "visual:test": "yarn run build-storybook:compose && playwright test",
    "build": "tsup && yarn build:angular && node scripts/copy-static-assets.mjs",
    "prebuild": "yarn run generate:tokens",
    "build:angular": "ng-packagr -p ng-package.json -c tsconfig.angular.json",
    "build:watch": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "svgo": "^3.0.2",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular/common": ">=18.0.0",
    "@angular/core": ">=18.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.21.2",
    "@angular-devkit/architect": "^0.1802.21",
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular-devkit/core": "^18.2.21",
    "@angular/animations": "18.0.1",
    "@angular/build": "^20.3.4",
    "@angular/cli": "^18.2.10",
    "@angular/common": "18.0.1",
    "@angular/compiler": "18.0.1",
    "@angular/compiler-cli": "18.0.1",
    "@angular/core": "18.0.1",
    "@angular/forms": "18.0.1",
    "@angular/platform-browser": "18.0.1",
    "@angular/platform-browser-dynamic": "18.0.1",
    "@changesets/cli": "^2.29.7",
    "@mdx-js/react": "^2.3.0",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-designs": "^10.0.2",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/angular": "^9.1.10",
    "@storybook/builder-vite": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@storybook/vue3-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^15.0.0",
    "@tokens-studio/sd-transforms": "^1.3.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.23",
    "@vue/compiler-sfc": "^3.5.12",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "http-server": "^14.1.1",
    "jsdom": "^24.1.0",
    "ng-packagr": "18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.0",
    "rxjs": "^7.8.1",
    "storybook": "^9.1.10",
    "style-dictionary": "^4.0.0",
    "tsup": "^8.1.0",
    "typescript": "~5.4.5",
    "vite": "^6.2.5",
    "vitest": "^2.0.4",
    "vue": "^3.5.12",
    "wait-on": "^7.2.0",
    "zone.js": "^0.14.7"
  }
}
